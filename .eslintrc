{
  "env": {
    "browser": true,
    "jest": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "extends": [
    "airbnb", "prettier"
  ],
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "no-console": 2,
    "camelcase": 0,
    "no-param-reassign": 0,
    "react/forbid-prop-types": 0,
    "import/prefer-default-export": 0
  }
}
